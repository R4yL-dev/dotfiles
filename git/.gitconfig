# Git Configuration
# Generated by dotfiles setup-git.sh
# Author: R4yL

[user]
	name = __GIT_USER_NAME__
	email = __GIT_USER_EMAIL__

[init]
	defaultBranch = main

[color]
	ui = auto

[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
